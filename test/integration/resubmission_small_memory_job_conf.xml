<?xml version="1.0"?>
<job_conf>
    <plugins>
        <plugin id="local" type="runner" load="galaxy.jobs.runners.local:LocalJobRunner" workers="1"/>
    </plugins>

    <destinations default="local_too_small">
        <destination id="local_too_small" runner="local">
            <env id="GALAXY_MEMORY_MB">4</env>
        </destination>
    </destinations>

</job_conf>
